    "import numpy as np\n",
    "\n",
    "def sigmoid(x):\n",
    "    return 1 / (1 + np.exp(-x))\n",
    "\n",
    "random_values = [-3.5, -1.2, 0, 2.8, -4.1, 1.5, -0.7, 3.2, -2.4, 4.6]\n",
    "\n",
    "for val in random_values:\n",
    "    print(f\"Sigmoid({val}) = {sigmoid(val)}\")\n"
   ]
